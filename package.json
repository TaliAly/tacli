{
  "name": "tacli",
  "version": "0.0.1",
  "main": "main.ts",
  "scripts": {
    "lint": "eslint --fix .",
    "start": "node --env-file=.env dist/main.js",
    "dev": "node --watch --env-file=.env dist/main.js",
    "build": "tsc --project tsconfig.json && tsconfig-replace-paths --project tsconfig.json",
    "build:watch": "tsc --project tsconfig.json && tsconfig-replace-paths --project tsconfig.json",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">9.0"
  },
  "dependencies": {
    "@ai-sdk/google": "0.0.27",
    "@inquirer/prompts": "5.1.2",
    "ai": "3.2.27",
    "clipboard": "2.0.11",
    "commander": "12.1.0",
    "dotenv": "16.4.5",
    "ollama-ai-provider": "0.10.0"
  },
  "devDependencies": {
    "@types/node": "20.14.10",
    "eslint": "8.57.0",
    "eslint-config-love": "56.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "tsconfig-replace-paths": "0.0.14",
    "typescript": "5.5.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint ."
    ]
  },
  "volta": {
    "node": "22.4.0"
  }
}
